{
    "editor.fontFamily": "'Operator Mono', 'Dank Mono', Consolas, 'Courier New', monospace",
    "workbench.iconTheme": "material-icon-theme",
    "glassit.alpha": 245,
    "editor.fontSize": 14,
    "workbench.sideBar.location": "left",
    "git.autofetch": true,
    "editor.fontLigatures": true,
    "editor.mouseWheelZoom": true,
    "debug.console.fontFamily": "Hack",
    "terminal.integrated.fontFamily": "Hack",
    "workbench.colorTheme": "βui - Ayu Dark",
    "editor.tokenColorCustomizations": {
        "[βui - Ayu Dark]": {
            "textMateRules": [
                {
                    "scope": "support.variable",
                    "settings": {
                        "foreground": "#e57373",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "variable.other.readwrite",
                    "settings": {
                        "foreground": "#fff8e1",
                        "fontStyle":""
                    }
                },
                {
                    "scope":"variable.parameter",
                    "settings": {
                        "foreground": "#e57373"
                    }
                },
                {
                    "scope":"variable.other",
                    "settings": {
                        "foreground": "#ffcc80",
                        "fontStyle": ""
                    }
                },
                {
                    "scope":"constant.character.escape",
                    "settings": {
                        "foreground": "#9175ff"
                    }
                },
                {
                    "scope": "meta",
                    "settings": {
                        "fontStyle":"",
                        "foreground": "#ffab40"
                    }
                },
                {
                    "scope": "punctuation.definition.string.begin",
                    "settings": {
                        "foreground": "#d4e157"
                    }
                },
                {
                    "scope": "punctuation.definition.string.end",
                    "settings": {
                        "foreground": "#d4e157"
                    }
                },
                {
                    "scope": "punctuation.definition.variable",
                    "settings":{
                        "fontStyle":""
                    }
                },
                {
                    "scope": "punctuation",
                    "settings": {
                        "foreground": "#9175ff"
                    }
                }
            ]
        }
    }
}
